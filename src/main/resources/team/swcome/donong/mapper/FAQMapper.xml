<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="team.swcome.donong.mapper.FAQMapper">
	
	<select id="selectAll" resultType="faq">
		select * from faq order by regitdate desc
	</select>
	
	<select id="selectPage" resultType="faq">
		select * from faq where num between 1 and 10 order by regitdate desc
	</select>
	
	<select id="select" resultType="faq" parameterType="int">
		select *from faq where num=#{num}
	</select>
	
	
	
</mapper>